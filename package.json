{
  "name": "yoga-with-adriene-bot",
  "version": "0.0.1",
  "description": "Gives you distraction-free yoga classes",
  "keywords": [],
  "repository": "https://github.com/oluckyman/YogaWithAdrieneBot",
  "author": "Ilya Belsky <ilya@belsky.in>",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "deploy": "node dist/index.js",
    "start": "rm -rd dist; concurrently -k -n TSC,HER -c blue,green \"npm run tsc:watch\" \"sleep 7; npm run local\"",
    "tsc:watch": "tsc -w",
    "local": "nodemon --watch dist --exec \"heroku local\" --signal SIGTERM",
    "lint": "eslint ."
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@amplitude/node": "^1.9.1",
    "@google-cloud/firestore": "^3.7.4",
    "bluebird": "^3.7.2",
    "d3-time-format": "^2.2.3",
    "dayjs": "^1.8.28",
    "dotenv": "^8.2.0",
    "fs": "0.0.1-security",
    "googleapis": "^49.0.0",
    "json-to-pretty-yaml": "^1.2.2",
    "lodash": "^4.17.20",
    "number-to-emoji": "^1.0.0",
    "request": "^2.88.2",
    "telegraf": "3.38.x",
    "written-number": "^0.9.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/d3-time-format": "^3.0.0",
    "@types/lodash": "^4.14.164",
    "@types/node": "^12.19.3",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
